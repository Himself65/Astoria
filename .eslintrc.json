{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2018
  },
  "extends": [
    "standard",
    "eslint:recommended"
  ],
  "rules": {
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "ignore"
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "warn"
  }
}
